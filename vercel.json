{
  "crons": [
    {
      "path": "/api/cron?secret=${CRON_SECRET}",
      "schedule": "0 12 * * *"
    },
    {
      "path": "/api/cron?secret=${CRON_SECRET}&discover=true",
      "schedule": "0 8 * * 0"
    }
  ],
  "env": {
    "NEWS_API_KEY": "@news_api_key",
    "GOOGLE_API_KEY": "@google_api_key",
    "MONGODB_URI": "@mongodb_uri",
    "CRON_SECRET": "@cron_secret"
  }
} 