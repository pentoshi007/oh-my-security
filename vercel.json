{
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 12 * * *"
    }
  ],
  "functions": {
    "apps/web/src/app/api/cron/route.ts": {
      "maxDuration": 60
    }
  },
  "env": {
    "NEWS_API_KEY": "@news_api_key",
    "GOOGLE_API_KEY": "@google_api_key",
    "MONGODB_URI": "@mongodb_uri",
    "CRON_SECRET": "@cron_secret"
  }
} 