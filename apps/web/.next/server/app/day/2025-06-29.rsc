import requests
import json
import os
import boto3
from botocore.exceptions import ClientError
# --- Example 1: Public Cloud Storage (S3 Bucket) Misconfiguration Check ---
# This script attempts to list contents of an S3 bucket to check for public accessibility.
# This directly relates to the "16 billion passwords leaked" scenario where data was
# likely exposed in misconfigured cloud storage.
def check_s3_bucket_public_access(bucket_name):
"""
Attempts to list objects in a given S3 bucket to check for public read access.
Requires AWS credentials configured (e.g., via AWS CLI `aws configure`)
or appropriate IAM roles if running on EC2/Lambda.
"""
print(f"\n--- Checking S3 Bucket: {bucket_name} for Public Access ---")
s3_client = boto3.client('s3')
try:
# Attempt to list objects. If successful without specific credentials,
# it indicates public read access or misconfigured IAM.
# For a truly 'public' check, we could try without any credentials,
# but boto3 typically uses configured ones. The key is if it succeeds
# on a bucket that *should* be private.
response = s3_client.list_objects_v2(Bucket=bucket_name, MaxKeys=1)
if 'Contents' in response:
print(f"[+] SUCCESS: Bucket '{bucket_name}' is accessible and contains objects.")
print(f"    First object: {response['Contents'][0]['Key']}")
print("    This indicates potential public read access or overly permissive IAM policies.")
else:
print(f"[+] SUCCESS: Bucket '{bucket_name}' is accessible but appears empty or has no listable contents.")
print("    Still indicates potential public read access if it should be private.")
except ClientError as e:
error_code = e.response['Error']['Code']
if error_code == 'AccessDenied':
print(f"[-] Access Denied: Bucket '{bucket_name}' is likely private or requires specific permissions.")
elif error_code == 'NoSuchBucket':
print(f"[-] Error: Bucket '{bucket_name}' does not exist.")
else:
print(f"[-] An unexpected AWS error occurred: {error_code} - {e}")
except Exception as e:
print(f"[-] An unexpected error occurred: {e}")
