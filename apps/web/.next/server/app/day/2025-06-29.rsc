# This script checks for common sensitive files and directories often exposed due to
# misconfigured web servers (e.g., allowing directory listings, exposing backup files).
# This relates to the Eclipse.org discussion where symlinks could expose sensitive files.
def check_web_server_exposure(target_url):
"""
Checks a web server for common misconfigurations leading to sensitive file exposure.
"""
print(f"\n--- Checking Web Server: {target_url} for Sensitive File Exposure ---")
common_paths = [
"/.git/config",          # Exposed Git repository configuration
"/.env",                 # Environment variables
"/phpinfo.php",          # PHP info page, often contains sensitive data
"/admin/",               # Default admin panels (check for directory listing)
"/backup.zip",           # Backup files
"/config.php.bak",       # Backup of configuration files
"/robots.txt",           # Sometimes contains disallowed paths that are actually sensitive
"/_wpeprivate/",         # WordPress Engine specific private directory
"/wp-config.php.bak",    # WordPress config backup
"/web.config.bak",       # IIS web config backup
"/CVS/",                 # CVS repository
"/WEB-INF/web.xml",      # Java web application configuration
"/server-status",        # Apache server status (if enabled and not restricted)
"/index.php~",           # Emacs/Vim backup files
"/index.html.bak",
"/.bash_history",        # If directory traversal is possible
"/.ssh/id_rsa",          # If directory traversal is possible
"/etc/passwd"            # If directory traversal is possible
]
headers = {'User-Agent': 'OhMySecurity-RedTeam-Scanner/1.0'}
for path in common_paths:
full_url = f"{target_url.rstrip('/')}{path}"
try:
response = requests.get(full_url, headers=headers, timeout=5)
if response.status_code == 200:
print(f"[+] Found Possible Exposure: {full_url} (Status: 200 OK)")
if "directory listing for" in response.text.lower() or "index of" in response.text.lower():
print(f"    [!] Directory Listing Enabled for {full_url}")
if len(response.text) > 100 and (path.endswith(".git/config") or ".env" in path or "phpinfo" in path):
print(f"    [!] Potentially Sensitive Content Found:")
print(f"        {response.text[:200]}...") # Print first 200 chars
elif response.status_code == 403:
print(f"[-] Access Denied: {full_url} (Status: 403 Forbidden)")
elif response.status_code == 404:
# print(f"[-] Not Found: {full_url} (Status: 404 Not Found)") # Too verbose
pass
else:
print(f"[?] Unexpected Status: {full_url} (Status: {response.status_code})")
except requests.exceptions.RequestException as e:
print(f"[-] Error accessing {full_url}: {e}")
